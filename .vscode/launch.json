  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Wordpress request",
      "type": "php",
      "request": "launch",
      "port": 9003,
      "pathMappings": {
        "/var/www/html/wp-content/plugins/mailbiz-woocommerce-tracker": "${workspaceFolder}/src",
        "/var/www/html/wp-content/plugins": "${workspaceFolder}/docker-volumes/plugins",
        "/var/www/html": "${workspaceFolder}/docker-volumes/wordpress",
      }
    },
    {
      "name": "Debug Pest tests",
      "type": "php",
      "request": "launch",
      "port": 9003,
      "pathMappings": {
        "/usr/app": "${workspaceFolder}",
      }
    }
  ]
}